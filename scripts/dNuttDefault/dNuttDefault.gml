function dNuttDefault(){
	myTextbox = noone;
	switch (global.character) {
		case oFezhead:
			icon[0] = sFezIcon1;
			text[0] = "Apparently, there's just a green sphere down here? How odd."
			icon[1] = sFezIcon2;
			text[1] = "Normally, I wouldn't care, but the face-on-a-rock principle is really getting to me."
			break;
		case oKidhead:
			icon[0] = sKidIcon1;
			text[0] = "Woah! You have the Burger Bros. official comic? I love that show!!"
			icon[1] = sNuttIcon;
			text[1] = "Pff, you really like that trash? Let's be honest with ourselves, the directors really dropped the ball with the anime adaptation. They totally wreck Nugget Neal's story arc, turning it from a pivotal<as>"
			icon[2] = sNuttIcon;
			text[2] = "...part of the McDonald's arc into a simple sidegag as they precede to totally flanderize Terry Tomato's character from a misguided vegetable to a simply vile charater. Pitiful, really."
			icon[3] = sKidIcon1;
			text[3] = "... it's just a kid's show, you know."
			icon[4] = sNuttIcon;
			text[4] = "Nonsense! I expect the highest tier writing in my fast food adventures!"
			break;
		case oConehead:
			icon[0] = sConeIcon1;
			text[0] = "who decided to throw their kickball into this random pit, then cover it up with a hidden plank of wood"
			icon[1] = sConeIcon1;
			text[1] = "and why did they draw a face on it? and why does it have a steel frame? how long has it been down here?"
			icon[2] = sConeIcon1;
			text[2] = "man, people around here are weird. you'd never see this kinda thing in the city."
			break;
	}
}